#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/.Trash-0/files/60c72796-5e50-475e-b6a9-aa589f40aa7f/node_modules/.pnpm/resolve@1.22.2/node_modules/resolve/bin/node_modules:/home/workspace/.Trash-0/files/60c72796-5e50-475e-b6a9-aa589f40aa7f/node_modules/.pnpm/resolve@1.22.2/node_modules/resolve/node_modules:/home/workspace/.Trash-0/files/60c72796-5e50-475e-b6a9-aa589f40aa7f/node_modules/.pnpm/resolve@1.22.2/node_modules:/home/workspace/.Trash-0/files/60c72796-5e50-475e-b6a9-aa589f40aa7f/node_modules/.pnpm/node_modules:/home/workspace/.Trash-0/files/60c72796-5e50-475e-b6a9-aa589f40aa7f/node_modules:/home/workspace/.Trash-0/files/node_modules:/home/workspace/.Trash-0/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/.Trash-0/files/60c72796-5e50-475e-b6a9-aa589f40aa7f/node_modules/.pnpm/resolve@1.22.2/node_modules/resolve/bin/node_modules:/home/workspace/.Trash-0/files/60c72796-5e50-475e-b6a9-aa589f40aa7f/node_modules/.pnpm/resolve@1.22.2/node_modules/resolve/node_modules:/home/workspace/.Trash-0/files/60c72796-5e50-475e-b6a9-aa589f40aa7f/node_modules/.pnpm/resolve@1.22.2/node_modules:/home/workspace/.Trash-0/files/60c72796-5e50-475e-b6a9-aa589f40aa7f/node_modules/.pnpm/node_modules:/home/workspace/.Trash-0/files/60c72796-5e50-475e-b6a9-aa589f40aa7f/node_modules:/home/workspace/.Trash-0/files/node_modules:/home/workspace/.Trash-0/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/resolve" "$@"
else
  exec node  "$basedir/../../bin/resolve" "$@"
fi
